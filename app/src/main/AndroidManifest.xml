            android:exported="true"/>
        <activity
            android:name=".LoginActivity"
            android:label="VETERINARIA CRISCAT"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".EmpezarActivity"
            android:label=""
            android:exported="true" />
        <activity
            android:name=".MantenedorMascotaActivity"
            android:label="MASCOTAS"
            android:exported="true" />
        <activity
            android:name=".ListaMascotaActivity"
            android:label="TUS MASCOTAS"
            android:exported="true" />
        <activity
            android:name=".MainActivity"
            android:exported="true" />
    </application>

</manifest>