        android:supportsRtl="true"
        android:theme="@style/Theme.CrisCatApp"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".ContactActivity"
            android:label="Sugerencias o Reclamaciones"
            android:exported="true" />
